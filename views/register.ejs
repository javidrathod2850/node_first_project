<html>
	<head>
		<% include include/header.ejs  %>
	</head>
    <body  class="landing">
    <% include include/menu.ejs  %>
    <section id="main" class="wrapper style1">
        <div class="container">
				<section id="content">
                    <section>
                        <h3>Register</h3>
                        <form method="post" action="insertdata">
                            <div class="row uniform">
                                <div class="6u 12u(3)">
                                    <input type="email" name="email" id="email" value="" placeholder="Email" />
                                </div>
                                <div class="6u 12u(3)">
                                    <input type="password" name="pwd" id="pwd" value="" placeholder="PAssword" />
                                </div>
                            </div>
                            <div class="row uniform">
                                <div class="12u">
                                    <div class="select-wrapper">
                                        <select name="country" id="country">
                                            <option value="">- Country -</option>
                                            <option value="1">India</option>
                                            <option value="2">England</option>
                                            <option value="3">USA</option>
                                            <option value="4">UK</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="row uniform">
                                <div class="4u 12u(2)">
                                    <input type="radio" id="male" name="gender" checked>
                                    <label for="male">Male</label>
                                </div>
                                <div class="4u 12u(2)">
                                    <input type="radio" id="female" name="gender">
                                    <label for="female">Female</label>
                                </div>
                             </div>
                            <div class="row uniform">
                                <div class="12u">
                                    <textarea name="address" id="address" placeholder="Enter your Address" rows="6"></textarea>
                                </div>
                            </div>
                            <div class="row uniform">
                                <div class="6u 12u(2)">
                                    <input type="checkbox" id="terms" name="terms">
                                    <label for="copy">Accept terms and condition.</label>
                                </div>
                            </div>
                            <div class="row uniform">
                                <div class="12u">
                                    <ul class="actions">
                                        <li><input type="submit" value="Register" /></li>
                                        <li><input type="reset" value="Reset" class="alt" /></li>
                                    </ul>
                                </div>
                            </div>
                        </form>
                    </section>
                </section>
        </div>
    </section>
    <% include include/footer.ejs %>
    </body>
</html>